<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core" >
	<Dialog
		id="saveTDialog"
		title="{i18n>saveTDialogText}">
		<content>
			<core:Icon
				src="sap-icon://accept"
				size="8rem"
				class="sapUiMediumMargin"/>
		</content>
		<beginButton>
			<Button
				text="{i18n>dialogCloseButtonText}"
				press=".onCloseDialog"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>