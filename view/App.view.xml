<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">
	<Shell>
		<App class="myAppDemoWT">
			<pages>
				<Page title="{i18n>createPageTitle}">
					<headerContent>
						<Button
							icon="sap-icon://home"
							press=".onOpenDialog"/>
						<Button
							icon="sap-icon://action"
							press=".onOpenDialog"/>
					</headerContent>
					<content>
						<mvc:XMLView viewName="sap.ui.demo.walkthrough.view.SurveyTitle"/>
						<mvc:XMLView viewName="sap.ui.demo.walkthrough.view.SurveyQuestion"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>